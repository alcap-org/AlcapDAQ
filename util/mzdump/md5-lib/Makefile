OBJS	= md5.o

CC	= cc
CFLAGS	= -g -I. -O2
LDFLAGS	=
RANLIB	= ranlib

LIBRARY	= libmd5.a

all : $(LIBRARY)

clean :
	rm -f md5 core *.o *.t *.cpp
	rm -f $(LIBRARY) $(TEST)

$(LIBRARY) : $(OBJS)
	ar cr $(LIBRARY) $?
	$(RANLIB) $@

.c.o :
	rm -f $@
	$(CC) $(CFLAGS) -c $< -o ./$@

#
# Below are dependencies that are automatically generated by make
# depend.  Please do not edit by hand.
#

md5.o: md5.c md5.h
